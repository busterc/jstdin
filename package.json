{
  "name": "jstdin",
  "version": "0.10.1",
  "description": "a better `node -p` that accepts stdin and can format and highlight output",
  "homepage": "https://github.com/busterc/jstdin",
  "author": {
    "name": "Buster Collings",
    "email": "busterc@gmail.com",
    "url": "https://about.me/buster"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "cli",
    "eval",
    "stdin",
    "node-p",
    "js",
    "json",
    "parse",
    "pretty-print",
    "jq",
    "jq.node",
    "fx",
    "npx"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-xo": "^0.24.2",
    "eslint-plugin-prettier": "^2.6.1",
    "execa": "^0.11.0",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "jest-cli": "^23.2.0",
    "lec": "^1.0.1",
    "lint-staged": "^7.2.0",
    "nsp": "^3.2.1",
    "prettier": "^1.13.7"
  },
  "scripts": {
    "prepublish": "lec lib/cli.js -c LF && nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "busterc/jstdin",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "ISC",
  "bin": "lib/cli.js",
  "dependencies": {
    "cardinal": "^2.1.1",
    "get-stdin": "^6.0.0",
    "meow": "^5.0.0"
  },
  "reveal": true
}
